<div class='container'>
  <%= search_form_for @q do |f| %>
    <!--name_contは、あいまい検索を指定-->
    <%= f.text_field :name_cont %>
    <%= f.submit "検索" %>
  <% end %>
  <div class='row'>
    <div class='col-md-3'><h2>User info</h2>
      <%= render 'info', user: current_user %>

      <h2 class="mt-3">New book</h2>
      <%= render 'books/form', book: @book %>
    </div>
    <div class='col-md-8 offset-md-1'>
      <h2>Users</h2>
      <%= render 'index',users: @users, ransack_users: @ransack_users %>
    </div>
  </div>
</div>